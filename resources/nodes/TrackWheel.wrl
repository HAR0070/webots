# A TrackWheel node can be used to simulate a wheel that it is part of a track system.

TrackWheel {
#fields specific to the TrackWheel node:
  vrmlField       SFVec2f    position     0 0
  hiddenField     SFRotation rotation     1 0 0 1.5708
  vrmlField       SFFloat    radius       0.1
  vrmlField       SFBool     inner        TRUE
#fields that inherit from the Group node:
  vrmlField   MFNode     children     []
}
